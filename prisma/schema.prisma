generator client {
	provider = "prisma-client-js"
}

datasource db {
	provider = "mysql"
	url      = env( "DB_URL" )
}

model GroceryItems {
	id				Int      	@id @unique @default( autoincrement() )
	name 			String 		@db.VarChar( 1000 ) @unique
}

model Settings {
	id				Int      	@id @unique @default( autoincrement() )
	name 			String 		@db.VarChar( 1000 ) @unique
	value 			String 		@db.VarChar( 1000 )
}


